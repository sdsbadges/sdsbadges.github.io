{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "member-access": false,
        "ordered-imports": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "radix": false,
        "max-line-length": [
            true,
            160
        ]
    },
    "rulesDirectory": []
}
